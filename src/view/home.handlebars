<h1>Productos</h1>

<div>
    {{#each products}}
    <ul>
        <li>{{this.title}}</li>
        <li>{{this.description}}</li>
        <li>{{this.price}}</li>
        <button>Agregar al carrito</button>
    </ul>
    {{/each}}
</div>

{{#if hasPrevPage}}
<a href="/?page={{prevPage}}">Anterior</a>
{{/if}}

{{#if hasNextPage}}
<a href="/?page={{nextPage}}">Siguiente</a>
{{/if}}

<p>{{currentPage}} de {{totalPages}}</p>